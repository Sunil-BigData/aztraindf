{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aztraindf"
		},
		"FileServervmlogs_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServervmlogs'"
		},
		"LS_Azsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Azsql'"
		},
		"azlakedatastore_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azlakedatastore'"
		},
		"azsypws_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azsypws'"
		},
		"FileServervmlogs_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\inetpub\\logs\\LogFiles\\W3SVC1"
		},
		"FileServervmlogs_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "vmadmin"
		},
		"azlakedatastore_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azlakedatastore.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aztraindf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Azsql_connectionString": {
						"value": "[parameters('LS_Azsql_connectionString')]"
					},
					"azlakedatastore_accountKey": {
						"value": "[parameters('azlakedatastore_accountKey')]"
					},
					"azsypws_connectionString": {
						"value": "[parameters('azsypws_connectionString')]"
					},
					"azlakedatastore_properties_typeProperties_url": {
						"value": "[parameters('azlakedatastore_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "aztraindf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FileServervmlogs_password": {
						"value": "[parameters('FileServervmlogs_password')]"
					},
					"FileServervmlogs_properties_typeProperties_host": {
						"value": "[parameters('FileServervmlogs_properties_typeProperties_host')]"
					},
					"FileServervmlogs_properties_typeProperties_userId": {
						"value": "[parameters('FileServervmlogs_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'aztraindf_ArmTemplate_0')]"
			]
		}
	]
}